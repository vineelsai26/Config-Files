{
	"name": "gitbackup",
	"packageManager": "yarn@3.3.0",
	"version": "1.0.0",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"init:ts": "tsc --init",
		"build": "tsc --outDir dist",
		"start": "node --trace-warnings dist/index.js",
		"watch": "tsc -w --outDir dist",
		"dev": "yarn watch | nodemon --watch dist dist/index.js"
	},
	"dependencies": {
		"adm-zip": "^0.5.10",
		"octokit": "^2.0.10"
	},
	"devDependencies": {
		"@types/adm-zip": "^0.5.0",
		"dotenv": "^16.0.3",
		"nodemon": "^2.0.20",
		"typescript": "^4.9.4"
	}
}
